          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="JSR223 Assertion">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import groovy.xml.*

def response = prev.getResponseDataAsString()
def xml = new XmlSlurper().parseText(response)

// Flag to check if match is found
def found = false

// Loop through all &lt;item&gt; nodes
xml.item.each { item -&gt;
    if (
        item.@category == &apos;Shooter&apos; &amp;&amp;
        item.id.text() == &apos;1&apos; &amp;&amp;
        item.name.text() == &apos;Resident Evil 4&apos;
    ) {
        found = true
    }
}

if (!found) {
    AssertionResult.setFailure(true)
    AssertionResult.setFailureMessage(&quot;Expected item not found: category=Shooter, id=1, name=Resident Evil 4&quot;)
}
</stringProp>
          </JSR223Assertion>
